  keywords = {out_of_obsidian,proj_purplmicroturbo},
  file = {C:\Users\abavkar\Zotero\storage\IY2XFVFS\Visser_et_al_Performance_Optimization_of_a_3kW_Microturbine_for_CHP_Applications.pdf}
