@inproceedings{Gaymann2017_DesignAdditiveManufacturing,
  title = {Design for {{Additive Manufacturing}}: {{Valves Without Moving Parts}}},
  shorttitle = {Design for {{Additive Manufacturing}}},
  booktitle = {Volume {{2C}}: {{Turbomachinery}}},
  author = {Gaymann, A. and Montomoli, F. and Pietropaoli, M.},
