@article{Braun2024_Rotatingdetonationcombustor,
  title = {Rotating Detonation Combustor Operability and Aero-Thermal Performance with an Integrated Diverging Nozzle},
  author = {Braun, James and Paniagua, Guillermo},
  date = {2024-07},
  journaltitle = {Applied Thermal Engineering},
