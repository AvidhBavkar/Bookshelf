@article{Pilkington2019_Predictionnaturalconvectiona,
  title = {Prediction of Natural Convection Heat Transfer in Gas Turbines},
  author = {Pilkington, Andrew and Rosic, Budimir and Tanimoto, Koichi and Horie, Shigenari},
  date = {2019-10-01},
