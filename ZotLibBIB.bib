  author = {Lu, Zifeng and Han, Jeongwoo and Wang, Michael and Cai, Hao and Sun, Pingping and Dieffenthaler, David and Gordillo, Victor and Monfort, Jean-Christophe and He, Xin and Przesmitzki, Steven},
