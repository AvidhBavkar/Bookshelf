@article{Eveker1998_IntegratedControlRotating,
  title = {Integrated {{Control}} of {{Rotating Stall}} and {{Surge}} in {{High-Speed Multistage Compression Systems}}},
  author = {Eveker, K. M. and Gysling, D. L. and Nett, C. N. and Sharma, O. P.},
  date = {1998-07-01},
  journaltitle = {Journal of Turbomachinery},
  shortjournal = {Journal of Turbomachinery},
