  shorttitle = {Understanding the Role of Contrails and Contrail Cirrus in Climate Change},
  author = {Singh, Dharmendra Kumar and Sanyal, Swarnali and Wuebbles, Donald J.},
  date = {2024-08-23},
  journaltitle = {Atmospheric Chemistry and Physics},
  volume = {24},
