@inproceedings{Marks2024_OnsetLowReynolds,
  title = {The {{Onset}} of {{Low Reynolds Number Separation}} in {{High-Lift High-Work LPT Passages}}},
  author = {Marks, Christopher R. and Fletcher, Nathan and Donovan, Molly and Clark, John P.},
  date = {2024-08-28},
  publisher = {American Society of Mechanical Engineers Digital Collection},
